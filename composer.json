{
  "name": "mitsuki/storage",
  "type": "library",
  "description": "A flexible and framework-agnostic storage library for managing structured records, originally built for the Mitsuki PHP framework and usable in any PHP application.",
  "license": "MIT",
  "keywords": [
    "mitsukio",
    "storage",
    "record-management",
    "fiches",
    "data-storage",
    "persistence",
    "php",
    "framework-agnostic",
    "modular",
    "backend"
  ],
  "autoload": {
    "psr-4": {
      "Mitsuki\\Storage\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "zgeniusecoders",
      "email": "zgeniusecoders@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require-dev": {
    "pestphp/pest": "^4.4",
    "mockery/mockery": "^1.6",
    "php": ">=8.0"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "symfony/filesystem": "^8.0",
    "symfony/http-foundation": "^8.0",
    "symfony/mime": "^8.0"
  }
}
